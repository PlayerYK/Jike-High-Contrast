{
  "manifest_version": 3,
  "name": "Jike High Contrast",
  "version": "1.0",
  "description": "提高即刻网页版的文字对比度，使文字更清晰易读",
  "permissions": ["activeTab"],
  "host_permissions": [
    "https://web.okjike.com/*"
  ],
  "content_scripts": [
    {
      "matches": ["https://web.okjike.com/*"],
      "css": ["styles.css"]
    }
  ],
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
